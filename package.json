{
  "name": "digitalbank-betaling-web",
  "version": "1.0.0",
  "description": "Et av prinsippene vi har fått fra arkitektur er at Digitalbanken deles inn i moduler som skal kunne utvikles separat og det skal være mulig å deploye en modul uten å påvirke andre. Derfor har vi skissert en løsning med relativt mange biter for å tilfredstille disse kravene.",
  "main": "gulpfile.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -r wwwroot/dist/*",
    "test": "npm run karma",
    "start": "npm run eslint-configs&&cross-env NODE_ENV=localhost webpack-dev-server --inline --hot --config webpack.config.development.js --port 7777 --progress --colors --content-base wwwroot/",
    "start-utv": "npm run eslint-configs&&cross-env NODE_ENV=localhost webpack-dev-server --inline --hot --config webpack.config.development.spvutv.js --progress --colors --content-base wwwroot/",
    "build-utvikling": "cross-env NODE_ENV=utvikling webpack --config webpack.config.production.js --colors",
    "build-systemtest": "cross-env NODE_ENV=systemtest webpack --config webpack.config.production.js --colors",
    "build-environments": "npm run build-utvikling && npm run build-systemtest",
    "eslint": "eslint *.js src/**/*.js",
    "eslint-configs": "eslint *.js",
    "eslint-teamcity": "eslint --format ./node_modules/eslint-teamcity/index.js *.js src/**/*.js",
    "karma": "npm run eslint-configs&&karma start",
    "karma-teamcity": "karma start --reporters teamcity  --bail true",
    "karma:debug": "npm run eslint-configs&&karma start --browsers Chrome --single-run=false",
    "analyze-bundle-s": "cross-env NODE_ENV=systemtest webpack --config webpack.config.production.js --json | webpack-bundle-size-analyzer",
    "analyze-bundle-u": "cross-env NODE_ENV=utvikling webpack --config webpack.config.production.js --json | webpack-bundle-size-analyzer"
  },
  "repository": {
    "type": "git",
    "url": "http://tfs.spv.no:8080/tfs/Utvikling/Spv-Git/_git/Selvbetjening.DigitalbankPoc.Konto"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "copyfiles": "^0.2.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.11.1",
    "eslint-loader": "^1.2.1",
    "eslint-teamcity": "^1.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.19.0",
    "ignore-loader": "^0.1.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "node-neat": "^1.7.2",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.9.1",
    "react-addons-perf": "^15.0.2",
    "react-addons-test-utils": "^15.1.0",
    "sass-loader": "^3.2.0",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.0",
    "wallaby-webpack": "0.0.22",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "yaml-loader": "^0.2.0"
  },
  "dependencies": {
    "classnames": "^2.1.4",
    "intl": "^1.2.4",
    "intl-messageformat": "^1.2.0",
    "intl-relativeformat": "^1.2.0",
    "lodash": "^4.13.1",
    "mobile-detect": "^1.3.2",
    "moment": "^2.13.0",
    "react": "15.1.0",
    "react-addons-pure-render-mixin": "^15.0.2",
    "react-day-picker": "^2.1.1",
    "react-dom": "15.1.0",
    "react-fastclick": "^2.1.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-form": "^5.2.5",
    "redux-thunk": "^2.0.1",
    "whatwg-fetch": "^1.0.0"
  }
}
